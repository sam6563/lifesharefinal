<h2><center>Log in</center></h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <center><%= f.label :email %></center>
    <center><%= f.email_field :email, autofocus: true %></center><br/>
  </div>

  <div class="field">
    <center><%= f.label :password %></center>
    <center><%= f.password_field :password, autocomplete: "off" %></center><br />
  </div>

  <div class="actions">
    <center><%= f.submit "Log in" %></center>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <center><%= f.label :remember_me %>&nbsp<%= f.check_box :remember_me %></center>
    </div>
  <% end -%>

<% end %>

<%= render "devise/shared/links" %>
